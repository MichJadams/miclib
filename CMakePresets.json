{
    "version": 3,
    "cmakeMinimumRequired": {
	"major": 4,
	"minor": 0,
	"patch": 3
    },
    "configurePresets": [
	{
	    "name": "default",
	    "displayName": "Default native build",
	    "generator": "Ninja",
	    "binaryDir": "${sourceDir}/build/",
	    "cacheVariables": {
		"CMAKE_EXPORT_COMPILE_COMMANDS": "YES"
	    }
	},
	{
	    "name": "windows-minGW",
	    "inherits": "default",
	    "generator": "MinGW Makefiles"
	}
    ],
    "buildPresets": [
	{
	    "name": "default",
	    "configurePreset": "default"
	}
    ]
}
